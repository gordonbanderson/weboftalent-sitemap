{
	"name": "weboftalent/sitemap",
	"description": "SilverStripe module to add a simple sitemap to your website",
	"type": "silverstripe-module",
	"keywords": [
		"silverstripe",
		"sitemap"
	],
	"authors": [{
		"name": "Gordon Anderson",
		"email": "gordon.b.anderson@gmail.com",
		"homepage": "https://github.com/gordonbanderson/weboftalent-sitemap",
		"role": "Developer"
	}],
	"support": {
		"issues": "https://github.com/gordonbanderson/weboftalent-sitemap/issues"
	},
    "require": {
    "php" : "~5.6|~7.0",
    "silverstripe/framework": "^4"
},
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Suilven\\PageSiteMap\\": "src/",
            "Suilven\\PageSiteMap\\Tests\\": "tests/"
        }
    },
	"license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true
}
